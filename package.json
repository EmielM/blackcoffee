{
  "name": "blackcoffee",
  "description": "CoffeeScript + hygienic macros",
  "author": "Frank van Viegen",
  "version": "0.2.0",
  "license": "MIT",
  "keywords": [
    "javascript",
    "language",
    "coffeescript",
    "macro",
    "compiler"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "lib": "./lib/coffee-script"
  },
  "main" : "./lib/coffee-script/coffee-script",
  "bin": {
    "coffee": "./bin/coffee",
    "blackcoffee":"./bin/blackcoffee",
    "cake": "./bin/cake"
  },
  "preferGlobal": true,
  "scripts": {
    "test":         "node ./bin/cake test",
    "test-harmony": "node --harmony ./bin/cake test"
  },
  "homepage": "https://github.com/paiq/blackcoffee",
  "bugs": "https://github.com/paiq/blackcoffee/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/paiq/blackcoffee.git"
  },
  "devDependencies": {
    "uglify-js": "~2.2",
    "jison": ">=0.2.0",
    "highlight.js": "~8.0.0",
    "underscore": "~1.5.2",
    "docco": "~0.6.2"
  },
  "dependencies": {
    "mkdirp": "~0.3.5"
  }
}
